@model List<PhotoAlbum.Models.People>

@{
    ViewData["Title"] = "People";
}

<h1>@ViewData["Title"]</h1>


@foreach (var people in Model)
{
<table>
    <thead>
        <tr>
            <td>Name</td>
            <td>LastName</td>
            <td>Gender</td>
            <td>Birthday</td>
            <td>Relation</td>
        </tr>
    </thead>
    <tr>
        <td>@people.Name</td>
        <td>@people.LastName</td>
        <td>@people.Gender</td>
        <td>@people.Birthday</td>
        <td>@people.Relation</td>
    </tr>
</table>
}

<a asp-controller="People" asp-action="Add">Add People</a>
